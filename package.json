{
  "name": "event-form",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:9000/",
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^7.2.1",
    "@types/express": "^4.17.8",
    "@types/jest": "^24.9.1",
    "@types/mongodb": "^3.5.27",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^12.12.62",
    "@types/react": "^16.9.50",
    "@types/react-datepicker": "^3.1.1",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.2",
    "email-validator": "^2.0.4",
    "express": "^4.17.1",
    "jest-canvas-mock": "^2.3.0",
    "mongodb": "^3.6.2",
    "mongoose": "^5.10.9",
    "msw": "^0.21.2",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-date-picker": "^8.0.3",
    "react-datepicker": "^3.2.2",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-scripts": "3.4.3",
    "redux": "^4.0.5",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "start": "concurrently \"npm run start-backend\" \"npm run start-frontend\"",
    "start-frontend": "react-scripts start",
    "start-backend": "ts-node --project backend/tsconfig.json backend/server.ts",
    "start-db": "mongod --dbpath data/db/ --auth",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "test:coverage": "npm test -- --coverage --watchAll=false --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.3",
    "@types/redux-mock-store": "^1.0.2",
    "canvas": "^2.6.1",
    "concurrently": "^5.3.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "redux-devtools": "^3.7.0",
    "redux-mock-store": "^1.5.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!backend/**/*",
      "!src/index.tsx",
      "!src/serviceWorker.ts",
      "!src/react-app-env.d.ts",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ]
  }
}
